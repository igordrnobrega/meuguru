<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.meuguru733478" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>MeuGuru</name>
  <description>
        Versão mobile do site Meu Guru
    </description>
  <author email="igordouradonobrega@gmail.com" href="http://meuguru.com.br/">
      Igor Nóbrega
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <access origin="*" launch-external="yes" />
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <icon src="icons/android/launcher-48x48px-android.png" width="48" height="48" />
  </platform>

  <platform name="ios">
    <icon src="icons/ios/launcher-48x48px-ios.png" width="76" height="76" />
    <icon src="icons/ios/launcher-48x48px-ios.png" width="152" height="152" />
  </platform>
</widget>